<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="360.0" prefWidth="420.0" style="-fx-background-color: #f2fafc;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
    <FlowPane fx:id="calendar" hgap="10.0" layoutX="10.0" layoutY="75.0" prefHeight="280.0" prefWidth="400.0" vgap="5.0" />
    <HBox alignment="CENTER" layoutX="160.0" layoutY="15.0" spacing="5.0">
        <children>
            <Button onAction="#backOneMonth" text="&lt;">
                <font>
                    <Font size="8.0" />
                </font>
            </Button>
            <Text fx:id="year" strokeType="OUTSIDE" strokeWidth="0.0" text="####">
            </Text>
            <Text fx:id="month" strokeType="OUTSIDE" strokeWidth="0.0" text="####">
            </Text>
            <Button onAction="#forwardOneMonth" text="&gt;">
                <font>
                    <Font size="8.0" />
                </font>
            </Button>
        </children>
    </HBox>
    <HBox alignment="CENTER" layoutX="10.0" layoutY="45.0" prefHeight="30.0" prefWidth="400.0" spacing="35.0">
        <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Sun" textAlignment="CENTER" />
            <Text layoutX="213.0" layoutY="37.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Mon" textAlignment="CENTER" />
            <Text layoutX="222.0" layoutY="37.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Tue" textAlignment="CENTER" />
            <Text layoutX="232.0" layoutY="37.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Wed" textAlignment="CENTER" />
            <Text layoutX="241.0" layoutY="37.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Thu" textAlignment="CENTER" />
            <Text layoutX="251.0" layoutY="37.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Fri" textAlignment="CENTER" />
            <Text layoutX="266.0" layoutY="37.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sat" textAlignment="CENTER" />
        </children>
    </HBox>
</AnchorPane>
